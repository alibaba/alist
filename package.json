{
  "name": "noformdocs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.6.4",
    "codemirror": "^5.37.0",
    "eventemitter2": "^5.0.1",
    "postcss-svgo": "^2.1.6",
    "prismjs": "^1.14.0",
    "react": "^16.3.2",
    "react-app-rewired": "^1.5.2",
    "react-codemirror2": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-markdown": "^3.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "svgo": "^1.0.5"
  },
  "homepage": "https://alibaba.github.io/noform/",
  "scripts": {
    "copyexample": "rm -rf public/examples && cp -r examples public/",
    "copydist": "cp -r dist public/dist",
    "exampleclean": "rm -rf examples/node_modules",
    "predeploy": "npm run copydist && npm run menu && npm run exampleclean && npm run copyexample && npm run build",
    "deploy": "gh-pages -d build",
    "gh": "gh-pages -d build",
    "start": "npm run menu && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "menu": "node scripts/menu.js",
    "sync": "./sync.sh",
    "ln": "./link.sh",
    "clean": "rm -rf dist && rm -rf examples/build && rm -rf examples/node_modules"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.8.0",
    "fs-extra": "^6.0.1",
    "gh-pages": "^1.1.0",
    "gitbook-parsers": "^0.8.9",
    "mkdirp": "^0.5.1",
    "react-app-rewire-less": "^2.0.8",
    "react-app-rewired": "^1.3.4",
    "rimraf": "^2.6.2"
  }
}
